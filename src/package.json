{
  "name": "panda.js-editor",
  "version": "0.1.0",
  "id": "net.pandajs.editor",
  "description": "Panda Editor",
  "main": "index.html",
  "window": {
    "width": 1280,
    "height": 720,
    "min_width": 740,
    "min_height": 378,
    "resizable": true,
    "show": false
  },
  "dependencies": {
    "pandajs": "latest"
  },
  "platformOverrides": {
    "win": {
      "window": {
        "toolbar": false
      }
    },
    "osx": {
      "window": {
        "toolbar": false
      }
    }
  }
}
